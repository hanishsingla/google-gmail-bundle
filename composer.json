{
    "name": "symfgenus/google-gmail-bundle",
    "type": "symfony-bundle",
    "description": "Provides a google gmail integration for your Symfony 3 Project.",
    "keywords": [
        "google gmail email"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Hanish",
            "email": "hksingla92@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "symfony/framework-bundle": "3.*",
        "google/apiclient": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "symfony/yaml": "~2.0",
        "twig/twig": "~1.12"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-0": {
            "Symfgenus\\GoogleGmailBundle\\": ""
        }
    },
    "target-dir": "Symfgenus/GoogleGmailBundle",
    "config": {
        "bin-dir": "bin/"
    }
}